<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="316.0" prefWidth="363.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pheonix.smiles.pheonix.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextField promptText="Phone Number or Email" />
   <PasswordField editable="false" prefHeight="26.0" prefWidth="323.0" promptText="Password" />
   <Button blendMode="MULTIPLY" onAction="#onHelloButtonClick" opacity="0.75" prefHeight="26.0" prefWidth="380.0" style="-fx-background-color: blue;" text="Log In" textFill="#ebebebf3">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Button>
  <Button blendMode="MULTIPLY" onAction="#onHelloButtonClick" opacity="0.75" prefHeight="26.0" prefWidth="338.0" style="-fx-background-color: d9d9d9;" text="Sign Up" textFill="#767676">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Button>

  <Label fx:id="welcomeText" />
</VBox>
